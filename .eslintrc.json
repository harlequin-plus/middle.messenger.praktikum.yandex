{
  "extends": [
    "airbnb",
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "max-len": [2, 100],
    "@typescript-eslint/no-unused-vars": 2,
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "allowImportingTsExtensions": 0
    }
} 