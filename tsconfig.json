{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2017",
    "declaration": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./typings",]
 },
} 
